<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <context:annotation-config/>

    <bean class="ua.com.shtramak.borisov.quoter.SenatorClayQuoter"/>

    <bean class="ua.com.shtramak.borisov.quoter.InjectRandomIntAnnotationBeanPostProcessor"/>

    <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor"/>

    <bean class="ua.com.shtramak.borisov.quoter.ProfilingHandlerBeanPostProcessor"/>

    <bean class="ua.com.shtramak.borisov.quoter.PostProxyInvokerContextListener"/>

</beans>